}

/* =============================================
   LISTA DE CITAS
   ============================================= */
QListWidget {
    background-color: #ffffff;
    border: 1px solid #d0d0d0;
    border-radius: 6px;
    color: #333333;
    font-size: 13px;
    padding: 4px;
}

QListWidget::item {
    padding: 10px 8px;
    border-radius: 4px;
    margin: 2px 0;
    border-left: 4px solid #ea6f15;
    background-color: #fafafa;
}

QListWidget::item:selected {
