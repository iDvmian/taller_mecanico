}

QCalendarWidget QAbstractItemView {
    background-color: #ffffff;
    color: #333333;
    selection-background-color: #ea6f15;
    selection-color: #ffffff;
    font-size: 12px;
}

QCalendarWidget QAbstractItemView:disabled {
