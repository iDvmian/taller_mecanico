    color: #ffffff;
}

QListWidget::item:hover:!selected {
    background-color: #f0f0f0;
}

/* =============================================
   SCROLLBAR
   ============================================= */
QScrollBar:vertical {
    background-color: #f0f0f0;
    width: 10px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background-color: #c0c0c0;
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background-color: #ea6f15;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0;
}

QScrollBar:horizontal {
    background-color: #f0f0f0;
    height: 10px;
    border-radius: 5px;
}

QScrollBar::handle:horizontal {
    background-color: #c0c0c0;
    border-radius: 5px;
    min-width: 30px;
}

QScrollBar::handle:horizontal:hover {
    background-color: #ea6f15;
}

/* =============================================
   BARRA DE ESTADO
   ============================================= */
QStatusBar {
    background-color: #f8f8f8;
    color: #666666;
    font-size: 11px;
    padding: 4px 12px;
    border-top: 2px solid #ea6f15;
}

/* =============================================
   TOOLTIPS
   ============================================= */
QToolTip {
    background-color: #ea6f15;
    color: #ffffff;
    border: none;
    padding: 6px 10px;
    border-radius: 4px;
    font-size: 12px;
}

/* =============================================
