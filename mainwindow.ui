/* =============================================
   TEMA CLARO - TALLER MECÁNICO
   ============================================= */

* {
    font-family: 'Segoe UI', Arial, sans-serif;
}

QMainWindow {
    background-color: #f5f5f5;
}

/* TÍTULO PRINCIPAL */
QLabel#titleLabel {
    font-size: 24px;
    font-weight: bold;
    color: #ea6f15;
    padding: 5px 0;
}

QLabel#subtitleLabel {
    font-size: 12px;
    color: #666666;
    padding-bottom: 5px;
}

/* =============================================
   PESTAÑAS
   ============================================= */
QTabWidget::pane {
    border: 1px solid #d0d0d0;
    background-color: #ffffff;
    border-radius: 6px;
    margin-top: -1px;
}

QTabBar::tab {
    background-color: #e8e8e8;
    color: #555555;
    padding: 10px 22px;
    margin-right: 2px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    font-size: 13px;
    font-weight: 600;
    min-width: 90px;
}

QTabBar::tab:selected {
    background-color: #ea6f15;
    color: #ffffff;
}

QTabBar::tab:hover:!selected {
    background-color: #d0d0d0;
}

/* =============================================
   TABLAS
   ============================================= */
QTableWidget {
    background-color: #ffffff;
    alternate-background-color: #fafafa;
    border: 1px solid #d0d0d0;
    border-radius: 6px;
    gridline-color: #e8e8e8;
    color: #333333;
    font-size: 13px;
    selection-background-color: #ea6f15;
    selection-color: #ffffff;
}

QTableWidget::item {
    padding: 8px;
    border-bottom: 1px solid #e8e8e8;
}

QTableWidget::item:selected {
    background-color: #ea6f15;
    color: #ffffff;
}

QTableWidget::item:hover {
    background-color: #f0f0f0;
}

QHeaderView::section {
    background-color: #f8f8f8;
    color: #ea6f15;
    padding: 10px 8px;
    border: none;
    border-bottom: 2px solid #ea6f15;
    font-weight: bold;
    font-size: 12px;
}

/* =============================================
   BOTONES PRINCIPALES
   ============================================= */
QPushButton {
    background-color: #ea6f15;
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 10px 20px;
    font-size: 13px;
    font-weight: bold;
    min-width: 100px;
}

QPushButton:hover {
    background-color: #f58025;
}

QPushButton:pressed {
    background-color: #d56010;
}

QPushButton:disabled {
    background-color: #cccccc;
    color: #888888;
}

/* Botones de Editar */
QPushButton#btnEditarCliente,
QPushButton#btnEditarOrden,
QPushButton#btnEditarRepuesto,
QPushButton#btnEditarCita {
    background-color: #9a6140;
    color: #ffffff;
}

QPushButton#btnEditarCliente:hover,
QPushButton#btnEditarOrden:hover,
QPushButton#btnEditarRepuesto:hover,
QPushButton#btnEditarCita:hover {
    background-color: #b07050;
}

/* Botones de Eliminar */
QPushButton#btnEliminarCliente,
QPushButton#btnEliminarVehiculo,
QPushButton#btnEliminarRepuesto,
QPushButton#btnEliminarCita {
    background-color: #c44444;
    color: #ffffff;
}

QPushButton#btnEliminarCliente:hover,
QPushButton#btnEliminarVehiculo:hover,
QPushButton#btnEliminarRepuesto:hover,
QPushButton#btnEliminarCita:hover {
    background-color: #d55555;
}

/* Botones Secundarios */
